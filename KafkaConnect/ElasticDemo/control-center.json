{
  "id": "/control-center",
  "acceptedResourceRoles": [
    "*"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 9021,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10002
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "confluentinc/cp-enterprise-control-center:3.3.1",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 2,
  "disk": 0,
  "env": {
    "CONTROL_CENTER_BOOTSTRAP_SERVERS": "broker.confluent-kafka.l4lb.thisdcos.directory:9092",
    "CONTROL_CENTER_ZOOKEEPER_CONNECT": "master.mesos:2181/dcos-service-confluent-kafka",
    "CONTROL_CENTER_INTERNAL_TOPICS_PARTITIONS": "3",
    "CONTROL-CENTER_MONITORING_INTERCEPTOR_TOPIC_PARTITIONS": "3",
    "CONTROL_CENTER_REPLICATION_FACTOR": "2",
    "CONTROL_CENTER_CONNECT_CLUSTER": "connect.marathon.l4lb.thisdcos.directory:8083"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 3600,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 10,
      "portIndex": 0,
      "timeoutSeconds": 5,
      "delaySeconds": 15,
      "protocol": "HTTP",
      "path": "/"
    }
  ],
  "instances": 1,
  "labels": {
    "DCOS_PACKAGE_OPTIONS": "e30=",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "HAPROXY_GROUP": "external",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_SERVICE_NAME": "control-center",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_VERSION": "1.0.0-3.3.1",
    "DCOS_PACKAGE_NAME": "confluent-control-center"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 4096,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}
