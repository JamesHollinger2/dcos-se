{
    "id": "/infra/network/k8sproxy-socket",
    "acceptedResourceRoles": [
      "slave_public"
    ],
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "container": {
      "portMappings": [
        {
          "containerPort": 10500,
          "hostPort": 10500,
          "protocol": "tcp",
          "servicePort": 10500
        }
      ],
      "type": "DOCKER",
      "volumes": [],
      "docker": {
        "image": "mesosphere/dcosappstudio:dcosappstudio-k8sProxy-v1.2.0",
        "forcePullImage": true,
        "privileged": false,
        "parameters": []
      }
    },
    "cpus": 0.25,
    "disk": 0,
    "env": {
      "HOSTS": "kube-node-0-kubelet.kubernetes.mesos:31500,kube-node-1-kubelet.kubernetes.mesos:31500,kube-node-2-kubelet.kubernetes.mesos:31500"
    },
    "healthChecks": [
    ],
    "instances": 1,
    "maxLaunchDelaySeconds": 3600,
    "mem": 256,
    "gpus": 0,
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "requirePorts": false,
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 300,
      "expungeAfterSeconds": 600
    },
    "fetch": [],
    "constraints": []
  }
